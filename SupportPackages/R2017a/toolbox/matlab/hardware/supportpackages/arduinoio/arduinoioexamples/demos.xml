<demos>
    <name>MATLAB Support Package for Arduino Hardware</name>
    <type>matlab</type>
    <icon>HelpIcon.DEMOS</icon>
    <description isCdata="no">
        <p>MATLAB&#174; lets you communicate with Arduino&#174; hardware. With this capability, you can use Arduino hardware as a physical I/O device.</p>
    </description>
    <website>
        <a href="http://www.mathworks.com/hardware-support/arduino-matlab.html">Product page at mathworks.com&#032;</a>
        <a href="http://www.mathworks.com/hardware-support/arduino-matlab.html"><img src="html/demo_arrow.png" border="none" alt="arrow" align="middle"></img></a>
    </website>
    <demosection>
        <label>Tutorials</label>
        <demoitem>
            <label>Getting Started with MATLAB Support Package for Arduino Hardware</label>
            <type>M-file</type>
            <source>ArduinoGettingStartedExample</source>
            <file>html/ArduinoGettingStartedExample.html</file>
        </demoitem>
        <demoitem>
            <label>Control Servo Motors</label>
            <type>M-file</type>
            <source>ServoMotorExample</source>
            <file>html/ServoMotorExample.html</file>
        </demoitem>
        <demoitem>
            <label>Measure Temperature From I2C Device on Arduino Hardware</label>
            <type>M-file</type>
            <source>I2CTemperatureSensorExample</source>
            <file>html/I2CTemperatureSensorExample.html</file>
        </demoitem>
        <demoitem>
            <label>Communicate with SPI Device on Arduino Hardware</label>
            <type>M-file</type>
            <source>SPIPotentiometerExample</source>
            <file>html/SPIPotentiometerExample.html</file>
        </demoitem>
        <demoitem>
            <label>Push Button Control with 74HC165 Shift Register</label>
            <type>M-file</type>
            <source>ShiftRegisterControlExample</source>
            <file>html/ShiftRegisterControlExample.html</file>
        </demoitem>
        <demoitem>
            <label>Control 7-Segment Display with 74HC595 Shift Register</label>
            <type>M-file</type>
            <source>ShiftRegisterDisplayExample</source>
            <file>html/ShiftRegisterDisplayExample.html</file>
        </demoitem>
        <demoitem>
            <label>Control LEDs with 74HC595 Shift Register</label>
            <type>M-file</type>
            <source>ShiftRegisterLEDExample</source>
            <file>html/ShiftRegisterLEDExample.html</file>
        </demoitem>
        <demoitem>
            <label>Control Motors Using Adafruit Motor Shield V2</label>
            <type>M-file</type>
            <source>AdafruitMotorShieldV2Example</source>
            <file>html/AdafruitMotorShieldV2Example.html</file>
        </demoitem>
        <demoitem>
            <label>Control Rotary Encoder Knob</label>
            <type>M-file</type>
            <source>RotaryEncoderKnobExample</source>
            <file>html/RotaryEncoderKnobExample.html</file>
        </demoitem>
        <demoitem>
            <label>Steer Two-Wheel Robot</label>
            <type>M-file</type>
            <source>RotaryEncoderSpeedExample</source>
            <file>html/RotaryEncoderSpeedExample.html</file>
        </demoitem>
    </demosection>  
</demos>

